<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="placa"
        class="br.gov.sp.fatec.Placa">
        <property name="mensagem" ref="mensagem"/>
    </bean>
    
    <bean id="mensagem"
        class="br.gov.sp.fatec.HelloWorld">
        <property name="nome" value="Sei la"/>
    </bean>
    
    <bean id="mensagem2"
        class="br.gov.sp.fatec.HelloWorld" scope="prototype">
        <property name="nome" value="Ze Ninguem"/>
    </bean>
    
    
    <bean id="usuario1"
        class="br.gov.sp.fatec.HelloWorldUsuario">
        <property name="nome" value="William"/>
        <property name="cpf" value="22233344455"/>
        <property name="idade" value="35"/>
        <property name="id" value="01"/>
        
    </bean>
    
    <bean id="usuario2"
        class="br.gov.sp.fatec.HelloWorldUsuario" scope="prototype">
        <property name="nome" value="Matheus"/>
        <property name="cpf" value="99988877766"/>
        <property name="idade" value="23"/>
        <property name="id" value="02"/>
    </bean>
    
    
   
    
</beans>
